?start: expr

?expr: expr "+" term   -> add
     | expr "-" term   -> sub
     | term

?term: term "*" factor -> mul
     | term "/" factor -> div
     | factor

?factor: INT        -> integer_number
       | "-" factor    -> neg
       | "(" expr ")"

%import common.NUMBER
%import common.INT
%import common.WS_INLINE
%ignore WS_INLINE